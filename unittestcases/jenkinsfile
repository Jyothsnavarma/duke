pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                dir('unittestcases') {
                    sh 'ls'
                    sh 'docker build -t unitcase -f Dockerfile .'
                }
            }
        }

        stage('Push to Docker Hub') {
            steps {
                    sh "docker login -u jyothsnadevi1401@gmail.com -p Nanagaru@0114"
                    sh 'docker tag unitcase jyothsnavarma/jyo:unitcase'
                    sh "docker push jyothsnavarma/jyo:unitcase"
                }
            }
        }

}
