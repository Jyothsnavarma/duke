pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                dir('unittestcases') {
                    sh 'ls'
                    sh 'docker build -t unittest -f Dockerfile .'
                }
            }
        }

        stage('Push to Docker Hub') {
            steps {
                    sh "docker login -u jyothsnadevi1401@gmail.com -p Nanagaru@0114"
                    sh 'docker tag unittest jyothsnavarma/jyo:unittest'
                    sh "docker push jyothsnavarma/jyo:unittest"
                }
            }
        }

}
