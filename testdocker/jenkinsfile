pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                dir('testdocker') {
                    sh 'ls'
                    sh 'docker build -t test -f Dockerfile .'
                }
            }
        }

        stage('Push to Docker Hub') {
            steps {
                    sh "docker login -u jyothsnadevi1401@gmail.com -p Nanagaru@0114"
                    sh 'docker tag test jyothsnavarma/jyo:test'
                    sh "docker push jyothsnavarma/jyo:test"
                }
            }
        }

}
