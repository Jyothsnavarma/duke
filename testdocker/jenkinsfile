pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                dir('testdocker') {
                    sh 'ls'
                    sh 'docker build -t socket -f Dockerfile .'
                }
            }
        }

        stage('Push to Docker Hub') {
            steps {
                    sh "docker login -u jyothsnadevi1401@gmail.com -p Nanagaru@0114"
                    sh 'docker tag flask jyothsnavarma/jyo:socket'
                    sh "docker push jyothsnavarma/jyo:socket"
                }
            }
        }

}
